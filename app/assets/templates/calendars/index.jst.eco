<h1>Conference Scheduler<h1>
<form id="search">
  <input type="text" name="room" id = "searchfield">
  <input type="submit" value="Search">
</form>
<div id="results">
<% if @rooms: %>
  <ul>
  <% for entry in @rooms: %>
    <li>
    <a href="#calendars/<%= entry.get('id')%>"><%= entry.get('name') %></a>
    </li>
  <% end %>
  </ul>
<% end %>
</div>
<div id="calendar">
</div>
